{{ define "response" }}
{
"@type": "dcat:Catalog",
"title": "City of San Diego catalog",
"homepage": "https://data.sandiego.gov",
"publisher": "City of San Diego Performance & Analytics Department",
"dataset" : [
{{ range $i, $e := .Data.Pages }}
    {{ if $i }}, {{ end }}
    {
        "@type": "dcat:Dataset",
        "title": {{ (.Title | title) | jsonify }},
        "description": {{ .Content | jsonify }},
        "issued": {{ .Params.date_issued | jsonify }},
        "keyword": null,
        "theme": {{ .Params.categories | jsonify }},
        "modified": {{ .Params.date_modified | jsonify }},
        "publisher": {{ .Params.departments | jsonify }},
        "accrualPeriodicity": null,
        "accessLevel": "public",
        "contactPoint": {{ .Params.maintainer_email | jsonify }},
        "bureauCode": null,
        "programCode": null,
        "license": {{ .Params.license | jsonify }},
        "distribution": [
          {{ range $index, $element := .Params.resources }}
            {{ if $index }}, {{ end }}
            {
                "title": {{ .name | jsonify }},
                "issued": null,
                "modified": null,
                "accessURL": null,
                "downloadURL": {{ .url | jsonify }},
                "byteSize": null,
                "format": {{ .format | jsonify }}
            }
          {{ end }}
        ]
    }
{{ end }}
]
}
{{ end }}